<UserControl x:Class="dms.gui.DecisionTreeInfoPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:dms.gui"
             mc:Ignorable="d" >
    <StackPanel>
        <Label ContentStringFormat="Название дерева решений: {0}">
            <Label x:Name="label" Content="Название дерева"/>
        </Label>
        <Label HorizontalAlignment="Center" Width="157">
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Width="120"/>
        </Label>
    </StackPanel>
</UserControl>
